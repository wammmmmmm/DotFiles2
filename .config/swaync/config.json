{
  "$schema": "/etc/xdg/swaync/configSchema.json",
  "positionX": "left",
  "positionY": "bottom",
  "layer": "overlay",
  "control-center-layer": "top",
  "layer-shell": true,
  "cssPriority": "user",
  "notification-icon-size": 50,
  "notification-body-image-height": 100,
  "notification-body-image-width": 200,
  "timeout": 10,
  "timeout-low": 5,
  "timeout-critical": 0,
  "fit-to-screen": true,
  "hide-on-clear": false,
  "hide-on-action": true,
  "script-fail-notify": true
}
